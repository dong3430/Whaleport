  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.or.ddit.user.reserve.meeting.mapper.MeetingMapper">
 	
 	<select id="list" resultType="kr.or.ddit.user.reserve.meeting.vo.MeetingVO">
